{
  "compilerOptions": {
    "moduleResolution": "node",
    "strict": true,
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "skipLibCheck": false,
    // "noEmit": true,
    "noLib": false,
    "jsx": "react",
    "rootDir": "./src",
    "outDir": "./dist",
    "sourceMap": true,
    "noImplicitAny": true,
    "module": "commonjs",
    "target": "es6"
  }
}
